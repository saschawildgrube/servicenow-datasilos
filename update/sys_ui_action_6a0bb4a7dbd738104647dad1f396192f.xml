        <name>DataSilos Access Analysis</name>
        <onclick>onDataSilosAccessAnalysis()</onclick>
        <order>25000</order>
        <script><![CDATA[function onDataSilosAccessAnalysis()
{
	var strUser = prompt('\nDataSilos Access Analysis:\nPlease enter a user\'s SysID or name:');
	if (strUser == null)
	{
		return;
	}
	
	var ga = new GlideAjax('DataSilosClientAPI');
	ga.addParam('sysparm_name','AccessAnalysisForUserAndRecord');
	ga.addParam('sysparm_user',strUser);
	ga.addParam('sysparm_table',g_form.getTableName());
	ga.addParam('sysparm_record',g_form.getUniqueValue());
	ga.getXML(CallbackResponse);
	function CallbackResponse(response)
	{
		var strResult = response.responseXML.documentElement.getAttribute('answer');
		alert('\nDataSilos Access Analysis:\n'+strResult);
	}
}]]></script>
